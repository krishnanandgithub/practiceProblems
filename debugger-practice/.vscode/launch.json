{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    
    

    {
      "request": "launch",
      "name": "Launch Program",
      "type": "node",
      "program": "${workspaceFolder}/code.js",
      "cwd": "${workspaceFolder}",
      "env": {},
      "runtimeExecutable": "/usr/local/bin/deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--inspect-brk",
        "--allow-all"
      ],
      "attachSimplePort": 9229"resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]L,

      "args": ["hello", "world"],
    }
    
  ]
}